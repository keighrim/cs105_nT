    <div class="row">
        <div class="col-md-3 col-md-offset-2">
            <% followers = @user.followers %>
            <b>followers:</b>
            <ul>
                <% followers.each do |f| %>
                <li><%="<a href=\"/profile/#{f.name}\"> #{f.name} </a>"%></br></li>
                <% end %>
            </ul>
        </div>
        <div class="col-md-3 col-md-offset-1">


            <% followed = @user.followed_users %>
            <b>followed users:</b>
            <ul>
                <% followed.each do |f| %>
                <li><%="<a href=\"/profile/#{f.name}\"> #{f.name} </a>"%></br></li>
                <% end %>
            </ul>
        </div>
    </div>
</div>